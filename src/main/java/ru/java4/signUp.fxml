    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <AnchorPane prefHeight="287.0" prefWidth="227.0">
            <children>
                <TextField fx:id="FirstName" layoutX="26.0" layoutY="29.0" promptText="First name" />
                <PasswordField fx:id="Password" layoutX="26.0" layoutY="127.0" promptText="Password" />
                <Button fx:id="closeButton" layoutX="32.0" layoutY="230.0" mnemonicParsing="false" onAction="#handleButtonAction2" prefHeight="27.0" prefWidth="160.0" text="Отмена" />
